@model Dictionary<String,List<String>>


<ul class="ListAulas">
@foreach (var item in Model.Keys)
{
    <li>
        <h4>Etapa  : @item</h4>
        <ul class="ListAulas">
            @foreach (var aula in Model[item])
            {
                <li><div>@Html.ActionLink("Aula " + aula, "../Aula/Index", new { titulo = aula }, new { @class = "classLink card classCard" })</div></li>
            }
        </ul>
    </li>
}
</ul>    
